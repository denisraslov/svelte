{
  "name": "storeon-svelte",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/distolma/storeon-svelte.git",
  "author": "Dmytro Mostovyi <djmakaron2009@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve example/index.html --open",
    "test": "jest --coverage && size-limit"
  },
  "devDependencies": {
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-svelte": "^3.0.1",
    "prettier": "^1.17.1",
    "size-limit": "^1.3.2",
    "svelte": "^3.4.0"
  },
  "dependencies": {
    "storeon": "^0.8.3"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "size-limit": [
    {
      "path": "index.js",
      "limit": "128 B"
    }
  ],
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "singleQuote": true
  }
}
